{
    "name": "mpyw/php-hyper-builtin-server",
    "description": "Reverse proxy for PHP built-in server which supports multiprocessing and TLS/SSL encryption",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "built-in",
        "builtin",
        "reverse",
        "proxy",
        "server",
        "TLS",
        "SSL",
        "multiprocess",
        "multiprocessing"
    ],
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "mpyw\\HyperBuiltinServer\\": "src/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "react/socket": "^0.4.3",
        "symfony/process": "^3.1"
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    },
    "bin": ["hyper-run"]
}
