{
    "name": "mpyw/HyperBuiltinServer",
    "description": "Reverse proxy for PHP built-in server which supports multiprocessing and TLS/SSL encryption",
    "type": "project",
    "license": "MIT",
    "keywords": [
        "built-in",
        "builtin",
        "reverse",
        "proxy",
        "server",
        "TLS",
        "SSL",
        "multiprocess",
        "multiprocessing"
    ],
    "authors": [
        {
            "name": "mpyw",
            "email": "ryosuke_i_628@yahoo.co.jp"
        }
    ],
    "autoload": {
        "psr-4": {
            "mpyw\\HyperBuiltinServer\\": "src/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "ext-posix": "*",
        "ext-pcntl": "*",
        "monolog/monolog": "^1.21",
        "psr/log": "^1.0"
    },
    "archive": {
        "exclude": [
            "*",
            ".*",
            "!/src/*"
        ]
    }
}
